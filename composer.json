{
    "name": "terriani/scoobyphp",
    "type" : "project",
	"homepage" : "https://github.com/terriani/ScoobyPHP",
	"description" : "Um framework MVC feito com PHP e muito amor para tornar o desnvolvimento web muito mais simples e divertido",
    "require":{
        "twig/twig": "^2.11",
        "illuminate/database": "^5.8",
        "larapack/dd": "^1.1",
        "matthiasmullie/minify": "1.*",
        "filp/whoops": "^2.5",
        "robmorgan/phinx": "0.10.0",
        "swiftmailer/swiftmailer": "^6.2",
        "dfridrich/php-mime-type": "^2.0",
        "jasongrimes/paginator": "^1.0",
        "coffeecode/optimizer": "^2.0",
        "kitetail/zttp": "^0.6.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload":{
        "psr-4": {
            "Scooby\\Helpers\\":"System/Helpers/",
            "Scooby\\Core\\":"System/Core/",
            "Scooby\\Controllers\\":"App/Controllers/",
            "Scooby\\Models\\":"App/Models/",
            "Scooby\\Services\\":"App/Services/",
            "Scooby\\Router\\":"System/Router/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsoftwig/twigcs": "^3.1",
        "fzaninotto/faker": "^1.8"
    }
}
